class Solution {
    public String solution(String my_string) {
        String answer = "";
        String charsToRemove = "aeiou";
        
        for (char c : charsToRemove.toCharArray()) {
            my_string = my_string.replaceAll(String.valueOf(c),"");
        }
        answer=my_string;
        
        return answer;
    }
}
